<?xml version="1.0" encoding="UTF-8" ?>

<object type="struct" name="XVLocation" desc="定位点">
	<object type="XVOptionalType" name="optional" desc="有效型" value="{0;1}" protype="enum"/>
	<object type="int" name="x" desc="X" protype="int"/>
	<object type="int" name="y" desc="Y" protype="int"/>
</object>

<object type="struct" name="XVPoint2DInt" desc="二维点整形">
	<object type="XVOptionalType" name="optional" desc="有效型" value="{0;1}" protype="enum"/>
	<object type="int" name="x" desc="X" protype="int"/>
	<object type="int" name="y" desc="Y" protype="int"/>
</object>

<object type="struct" name="XVBox" desc="Box">
	<object type="XVOptionalType" name="optional" desc="有效型" value="{0;1}" protype="enum"/>
	<object type="XVPoint2D" name="origin" desc="顶点" memo="顶点(左上点，不是中心)"/>
	<object type="int" name="width" desc="宽度"/>
	<object type="int" name="height" desc="高度"/>
</object>

<object type="struct" name="XVPixel" desc="像素">
	<object type="XVOptionalType" name="optional" desc="有效型" canlink="false"/>
	<object type="float" name="x" desc="x"/>
	<object type="float" name="y" desc="y"/>
	<object type="float" name="z" desc="z"/>
	<object type="float" name="w" desc="w"/>
</object>

<object type="struct" name="XVSize" desc="尺寸">
	<object type="XVOptionalType" name="optional" desc="有效型" value="{0;1}" protype="enum"/>
	<object type="int" name="width" desc="宽" protype="int"/>
	<object type="int" name="y" desc="高" protype="int"/>
</object>
